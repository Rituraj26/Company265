{
  "name": "homepage_assignment",
  "version": "1.0.0",
  "description": "Homepage Assignment landing page",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass ./stylesheets/sass/main.scss ./stylesheets/css/style.css -w",
    "devserver": "live-server --browser=firefox",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass ./stylesheets/sass/main.scss ./stylesheets/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./stylesheets/css/style.comp.css -o ./stylesheets/css/style.prefix.css",
    "compress:css": "node-sass ./stylesheets/css/style.prefix.css ./stylesheets/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Rituraj",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  },
  "dependencies": {}
}
